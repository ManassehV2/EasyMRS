<!doctype html>

<html class="no-js" lang="">
<head>
    <title>Easy MRS</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="apple-touch-icon" href="apple-icon.png">
    <link rel="shortcut icon" href="favicon.ico">
    <link href="~/Content/assets/css/normalize.css" rel="stylesheet" />
    <link href="~/Content/assets/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/assets/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/assets/css/themify-icons.css" rel="stylesheet" />
    <link href="~/Content/assets/css/flag-icon.min.css" rel="stylesheet" />
    <link href="~/Content/assets/css/cs-skin-elastic.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/Content/assets/css/lib/datatable/dataTables.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/assets/scss/style.css" rel="stylesheet" />
    <!-- <link rel="stylesheet" href="assets/css/bootstrap-select.less"> -->

    <link href="~/Content/smart_wizard_theme_arrows.css" rel="stylesheet" />
    <link href="~/Content/smart_wizard.css" rel="stylesheet" />

    <link href="~/Content/assets/scss/style.css" rel="stylesheet" />
    <link href="~/Content/assets/css/lib/vector-map/jqvmap.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,700,800' rel='stylesheet' type='text/css'>
    <!-- <script type="text/javascript" src="https://cdn.jsdelivr.net/html5shiv/3.7.3/html5shiv.min.js"></script> -->
</head>
<body>

    <!-- Left Panel -->
    <aside id="left-panel" class="left-panel">
        <nav class="navbar navbar-expand-sm navbar-default">
            <div class="navbar-header">
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#main-menu" aria-controls="main-menu" aria-expanded="false" aria-label="Toggle navigation">
                    <i class="fa fa-bars"></i>
                </button>
                <a class="navbar-brand" href="./">Easy MRS</a>
                <a class="navbar-brand hidden" href="./">MRS</a>
            </div>
            <div id="main-menu" class="main-menu collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li class="active">
                        <a href="index.html"> <i class="menu-icon fa fa-dashboard"></i>Dashboard </a>
                    </li>
                    <h3 class="menu-title">General Actions</h3><!-- /.menu-title -->
                    <li>
                       
                            <a href="#"></a>
                            <a data-toggle="modal" data-target="#staticModalstartVisit">start visit</a>
                            <a href="#">Add past visit</a>
                            <a href="#">Schedule Appointment</a>
                            <a href="#">Request Appointment</a>
                            <a data-toggle="modal" data-target="#staticModaldeciced">Mark Ptient Deceased</a>
                            <a data-toggle="modal" data-target="#staticModalDelete">Delete Patient</a>
                            
</li>
                    
                    <h3 class="menu-title">Other Actions</h3><!-- /.menu-title -->
                </ul>
            </div><!-- /.navbar-collapse -->
        </nav>
    </aside><!-- /#left-panel -->
    <!-- Left Panel -->
    <!-- Right Panel -->
    <div id="right-panel" class="right-panel">
        <!-- Header-->
        <header id="header" class="header">
            <div class="header-menu">
                <div class="col-sm-7">
                    <a id="menuToggle" class="menutoggle pull-left"><i class="fa fa fa-tasks"></i></a>
                    <div class="header-left">
                        <button class="search-trigger"><i class="fa fa-search"></i></button>
                        <div class="form-inline">
                            <form class="search-form">
                                <input class="form-control mr-sm-2" type="text" placeholder="Search ..." aria-label="Search">
                                <button class="search-close" type="submit"><i class="fa fa-close"></i></button>
                            </form>
                        </div>
                        
                        
                    </div>
                </div>
                <div class="col-sm-5">
                    <div class="user-area dropdown float-right">
                            @Html.Partial("LogInStatus")
                    </div>
                    
                </div>
            </div>
        </header><!-- /header -->
        <!-- Header-->
        <div class="breadcrumbs">
            <div class="col-sm-4">
                <div class="page-header float-left">
                    <div class="page-title">
                        <h1>General Actions</h1>
                    </div>
                </div>
            </div>
            <div class="col-sm-8">
                <div class="page-header float-right">
                    <div class="page-title">
                        <ol class="breadcrumb text-right">
                            <li class="active">Dashboard</li>
                        </ol>
                    </div>
                </div>
            </div>
        </div>
        <div class="content mt-3">
            @RenderBody()
        </div> <!-- .content -->
    </div><!-- /#right-panel -->
    <!-- Right Panel -->

    <script src="~/Content/assets/js/vendor/jquery-2.1.4.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.3/umd/popper.min.js"></script>
    <script src="~/Content/assets/js/plugins.js"></script>
    <script src="~/Content/assets/js/main.js"></script>
    <script src="~/Content/assets/js/lib/chart-js/Chart.bundle.js"></script>
    <script src="~/Content/assets/js/dashboard.js"></script>
    <script src="~/Content/assets/js/widgets.js"></script>
    <script src="~/Content/assets/js/lib/vector-map/jquery.vmap.js"></script>
    <script src="~/Content/assets/js/lib/vector-map/jquery.vmap.min.js"></script>
    <script src="~/Content/assets/js/lib/vector-map/jquery.vmap.sampledata.js"></script>
    <script src="~/Content/assets/js/lib/vector-map/country/jquery.vmap.world.js"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>

    <script src="~/Scripts/jquery.smartWizard.js"></script>
    <script>
        (function ($) {
            "use strict";

            jQuery('#vmap').vectorMap({
                map: 'world_en',
                backgroundColor: null,
                color: '#ffffff',
                hoverOpacity: 0.7,
                selectedColor: '#1de9b6',
                enableZoom: true,
                showTooltip: true,
                values: sample_data,
                scaleColors: ['#1de9b6', '#03a9f5'],
                normalizeFunction: 'polynomial'
            });
        })(jQuery);
    </script>
</body>
</html>
